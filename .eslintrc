{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "semi": "off",
      "linebreak-style": ["warn", "windows"],
      "no-console": "off",
      "no-underscore-dangle": "off",
      "indent": ["error", 2]
    }
  }
  